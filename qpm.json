{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.4.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Transitions",
    "id": "transitions",
    "version": "1.2.3",
    "url": null,
    "additionalData": {
      "overrideSoName": "libtransitions.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1 $0?"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?",
        "pwsh ./scripts/restart-game.ps1"
      ],
      "log": [
        "pwsh ./scripts/start-logging.ps1 $0:"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 $0?",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1 $0?"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 -analyze"
      ]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": [
      "build",
      "extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "./Transitions.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^4008.0.0",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.53",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^2.0.3",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    }
  ]
}
